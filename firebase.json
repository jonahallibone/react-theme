{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/projects",
        "destination": "/work",
        "type": 301
      },
      {
        "source": "/projects/:slug*",
        "destination": "/work/:slug",
        "type": 301
      },
      {
        "source": "/projects/new-york-university-60-fifth-avenue/",
        "destination": "/work/nyu-advanced-research-institutes",
        "type": 301
      },
      {
        "source": "/VAP{,/**}",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/posters{,/**}",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/VAP/dh_poster.html",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/studio",
        "destination": "/practice",
        "type": 301
      },
      {
        "source": "/admin",
        "destination": "https://api.piscatello.com/wp-admin",
        "type": 301
      }
    ]
  }
}
